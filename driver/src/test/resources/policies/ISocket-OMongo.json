{
  "name": "ATSocketMongo",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ],
  "fields": [
    {
      "name": "product"
    },
    {
      "name": "price"
    },

    {
      "name": "timestamp",
      "type": "DateTime",
      "configuration": {
        "typeOp": "String"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "dimensions": [
        {
          "dimension": "product"
        }
      ],
      "operators": [
        {
          "measureName": "sum-operator",
          "type": "Sum",
          "configuration": {
            "inputField":"price"
          }
        },
        {
          "measureName": "avg-operator",
          "type": "Avg",
          "configuration": {
            "inputField":"price"
          }
        }
      ]

    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "clientUri": "mongodb://localhost:60000",
        "dbName": "csvtest",
        "multiplexer": "false"
      }
    }
  ],
  "parsers": [
  {
    "name": "morphline-parser",
    "type": "Morphlines",
    "configuration": {
      "morphline": {
        "id": "morphline1",
        "importCommands": [
          "org.kitesdk.**"
        ],
        "commands" : [
          {
            "readCSV" : {
              "separator" : ",",
              "columns" : ["product","price"],
              "trim" : "true",
              "charset" : "UTF-8",
              "quoteChar" : "\"",
              "commentPrefix" : "#"
            }
          }
        ]
      }
    }
  }
]
}