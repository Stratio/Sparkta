               "width": 4,
               "qa": "custom-transform-properties-key"
            },
            {
               "propertyId": "transformationOptionsValue",
               "propertyName": "_OPTION_VALUE_",
               "propertyType": "text",
               "required": false,
               "width": 4,
               "qa": "custom-transform-properties-value"
            }
