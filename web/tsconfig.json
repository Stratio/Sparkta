{
   "angularCompilerOptions": {
      "genDir": "./dist/aot"
   },
   "buildOnSave": false,
   "compileOnSave": false,
   "compilerOptions": {
      "baseUrl": "./src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": ["es2015", "dom"],
      "mapRoot": "",
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "dist",
      "paths": {
         "@app/*": ["app/modules/*"],
         "reducers": [
            "./app/reducers"
         ],
         "actions/*": [
            "./app/actions/*"
         ],
         "models/*": [
            "./app/models"
         ],
         "services/*": [
            "./app/services"
         ],
         "services": [
            "./app/services"
         ],
         "utils": [
            "./app/utils"
         ],
         "data-templates/*": [
            "./app/data-templates/*"
         ],
         "assets/*": [
             "./assets/*"
         ],
         "constants/*": [
            "./app/modules/shared/constants/*"
         ]
      },
      "rootDir": ".",
      "skipLibCheck": true,
      "sourceMap": true,
      "target": "es5",
      "types": ["jasmine", "node"]
   },
   "exclude": ["dist", "node_modules"],
   "include": [
      "src/**/*"
   ]
}
