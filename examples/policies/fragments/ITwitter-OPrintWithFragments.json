{
  "name": "Policy-Twitter-Print",
  "description": "Input Twitter, output Print",
  "sparkStreamingWindow": 6000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "fragments": [
    {
      "id": "change with the id of the fragment",
      "name": "Twitter",
      "fragmentType": "input",
      "description": "description",
      "shortDescription": "shortDescription",
      "icon": "icon",
      "element": null
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "name": "hashtags",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "hashtags"
        },
        {
          "name": "firsthashtag",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "firsthashtag"
        },
        {
          "name": "retweets",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "retweets"
        },
        {
          "name": "userLocation",
          "field": "userLocation"
        },
        {
          "name": "precision3",
          "field": "geolocation",
          "type": "GeoHash",
          "precision": "precision3"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        },
        {
          "name": "distinctoperator",
          "type": "Count",
          "configuration": {
            "distinctFields": "wordsN"
          }
        },
        {
          "name": "sumoperator",
          "type": "Sum",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "maxoperator",
          "type": "Max",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "minoperator",
          "type": "Min",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "avgoperator",
          "type": "Avg",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "medianoperator",
          "type": "Median",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "varianceoperator",
          "type": "Variance",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "stddevoperator",
          "type": "Stddev",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "fullTextoperator",
          "type": "FullText",
          "configuration": {
            "inputField": "userLocation"
          }
        },
        {
          "name": "lastValueoperator",
          "type": "LastValue",
          "configuration": {
            "inputField": "retweets"
          }
        },
        {
          "name": "accumulatoroperator",
          "type": "Accumulator",
          "configuration": {
            "inputField": "wordsN"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {
        "multiplexer": "true",
        "isAutoCalculateId": "true"
      }
    }
  ]
}
