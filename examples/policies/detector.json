{
  "name": "policy-kafka",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "true",
    "timePartition": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-kafka",
      "type": "Kafka",
      "configuration": {
        "topics": "detectorStream3",
        "kafkaParams.zookeeper.connect": "****",
        "kafkaParams.group.id": "testgroup",
        "storageLevel": "MEMORY_AND_DISK_SER_2"
      }
    }
  ],
  "fields": [
    {
      "name": "company_root"
    },
    {
      "name": "ou_vehicle"
    },
    {
      "name": "asset"
    },
    {
      "type": "DateTime",
      "name": "recorded_at_ms"
    },
    {
      "type": "GeoHash",
      "name": "geo"
    },
    {
      "name": "rpm_event_avg"
    },
    {
      "name": "odometer"
    },
    {
      "name": "odometerNum"
    },
    {
      "name": "rpmAvgNum"
    },
    {
      "name": "path_id"
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "dimensions": [
        {
          "dimension": "company_root"

        },
        {
          "dimension": "ou_vehicle"

        },
        {
          "dimension": "geo",
          "precision": "precision12"
        },
        {
          "dimension": "recorded_at_ms",
          "precision": "minute"
        }
      ],
      "operators": [
        {
        "measureName": "avg-operator",
        "type": "Avg",
        "configuration": {
          "inputField": "rpmAvgNum"
        }
      }
      ]
    },
    {
      "name": "testCube1",
      "dimensions": [
        {
          "dimension": "company_root"

        },
        {
          "dimension": "ou_vehicle"

        },
        {
          "dimension": "asset"

        },
        {
          "dimension": "recorded_at_ms",
          "precision": "minute"
        },
        {
          "dimension": "path_id",

        }
      ],

      "operators": [
        {
        "measureName": "max-operator",
        "type": "Max",
        "configuration": {
          "inputField": "odometerNum"
        }
      },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "odometerNum"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "clientUri": "mongodb://localhost:27017",
        "dbName": "sparkta",
        "multiplexer": "false",
        "granularity": "minute",
	    "identitiesSaved": "true",
        "idAsField": "true"
      }
    }
  ],

  "parsers": [
    {
      "name": "detector-parser",
      "type": "Detector",
      "configuration": {
      }
    },
    {
      "name": "recorded_at_ms-parser",
      "type": "DateTime",
      "configuration": {
        "recorded_at_ms": "unixMillis"
      }
    }
  ]
}
