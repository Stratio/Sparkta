{
  "name": "policy-Kafka-Mongo-Zion",
  "sparkStreamingWindow": 6000,
  "rawData": {
    "enabled": "false",
    "timePartition": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-kafka",
      "type": "Kafka",
      "configuration": {
        "topics": "zion2:1",
        "kafkaParams.zookeeper.connect": "localhost:2181",
        "kafkaParams.group.id": "kafka-pruebas",
        "storageLevel": "MEMORY_AND_DISK_SER_2"
      }
    }
  ],
  "fields": [
    {
      "name": "appName"
    },
    {
      "name": "appCountry"
    },
    {
      "name": "appPlatform"
    },
    {
      "name": "appVersion"
    },
    {
      "name": "device"
    },
    {
      "name": "osVersion"
    },
    {
      "name": "lang"
    },
    {
      "name": "appLang"
    },
    {
      "type": "DateTime",
      "name": "date"
    },
    {
      "type": "DateTime",
      "name": "timestamp"
    },
    {
      "name": "session"
    },
    {
      "name": "extra1"
    },
    {
      "name": "extra2"
    },
    {
      "name": "extra3"
    },
    {
      "name": "source"
    },
    {
      "name": "responseTime"
    },
    {
      "name": "environment"
    },
    {
      "name": "platform"
    },
    {
      "type": "GeoHash",
      "name": "geo"
    }
  ],
  "cubes": [
    {
      "name": "time",
      "dimensions": [
        {
          "dimension": "timestamp",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    },
    {
      "name": "session",
      "dimensions": [
        {
          "dimension": "session"
        },
        {
          "dimension": "timestamp",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    },
    {
      "name": "platform",
      "dimensions": [
        {
          "dimension": "platform"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    },
    {
      "name": "device",
      "dimensions": [
        {
          "dimension": "device"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    },
    {
      "name": "geo",
      "dimensions": [
        {
          "dimension": "geo",
          "precision": "precision6"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    },
    {
      "name": "appPlatform",
      "dimensions": [
        {
          "dimension": "appPlatform"
        },
        {
          "dimension": "timestamp",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "responseTime"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "responseTime"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "clientUri": "mongodb://localhost:27017",
        "dbName": "sparkta",
        "multiplexer": "false"
      }
    }
  ],
  "parsers": [
    {
      "name": "morphline-parser",
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**",
            "com.stratio.ingestion.morphline.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "appName": "/appName",
                  "method": "/method",
                  "datetime": "/date",
                  "appCountry": "/appCountry",
                  "appPlatform": "/appPlatform",
                  "appVersion": "/appVersion",
                  "uid": "/uid",
                  "device": "/device",
                  "latitude": "/latitude",
                  "longitude": "/longitude",
                  "osVersion": "/osVersion",
                  "lang": "/lang",
                  "appLang": "/appLang",
                  "user_id": "/user_id",
                  "connection": "/connection",
                  "timestamp": "/timestamp",
                  "session": "/session",
                  "extra1": "/extra1",
                  "extra2": "/extra2",
                  "extra3": "/extra3",
                  "source": "/source",
                  "environment": "/environment",
                  "platform": "/platform",
                  "responseTime": "/responseTime"
                }
              }
            },
            {
              "addValues": {
                "geo": "@{latitude}__@{longitude}"
              }
            },
            {
              "removeFields": {
                "blacklist": [
                  "literal:_attachment_body",
                  "literal:message"
                ]
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "type": "DateTime",
      "configuration": {
        "timestamp": "autoGenerated"
      }
    }
  ]
}