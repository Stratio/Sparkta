{
  "name": "policy-Twitter-Redis",
  "duration": 10000,
  "saveRawData": "false",
  "rawDataParquetPath": "myTestParquetPath",
  "checkpointDir": "checkpoint",
  "timeBucket": "minute",
  "checkpointGranularity": "minute",
  "checkpointInterval": 30000,
  "checkpointTimeAvailability": 60000,
  "inputs": [
    {
      "name": "in-twitter",
      "elementType": "TwitterInput",
      "configuration": {
        "consumerKey": "*****",
        "consumerSecret": "*****",
        "accessToken": "*****",
        "accessTokenSecret": "*****"
      }
    }
  ],
  "dimensions": [
    {
      "dimensionType": "TwitterStatusDimension",
      "name": "status"
    },
    {
      "name": "wordsN"
    },
    {
      "dimensionType": "DateTimeDimension",
      "name": "timestamp"
    },
    {
      "dimensionType": "GeoHashDimension",
      "name": "geolocation"
    }
  ],
  "cubes": [
    {
      "name": "twitter",
      "dimensions": [
        {
          "dimension": "status",
          "precision": "hastags"
        },
        {
          "dimension": "status",
          "precision": "firsthastag"
        },
        {
          "dimension": "status",
          "precision": "retweets"
        },
        {
          "dimension": "geolocation",
          "precision": "precision3"
        },
        {
          "dimension": "timestamp",
          "precision": "minute"
        }
      ],
      "operators": ["count-operator","max-operator","min-operator"]
    }
  ],
  "outputs": [
    {
      "name": "out-redis",
      "elementType": "RedisOutput",
      "configuration": {
        "hostname": "localhost",
        "multiplexer": "true"
      }
    }
  ],
  "operators": [
    {
      "name": "count-operator",
      "elementType": "CountOperator",
      "configuration": {}
    },
    {
      "name": "max-operator",
      "elementType": "MaxOperator",
      "configuration": {
        "inputField": "wordsN"
      }
    },
    {
      "name": "min-operator",
      "elementType": "MinOperator",
      "configuration": {
        "inputField": "wordsN"
      }
    }
  ]
}