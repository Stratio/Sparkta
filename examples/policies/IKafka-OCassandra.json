{
  "name": "policy-Kafka-Cassandra",
  "sparkStreamingWindow": 6000,
  "rawData": {
    "enabled": "false",
    "timePartition": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-kafka",
      "type": "Kafka",
      "configuration": {
        "topics": "streaming_data",
        "kafkaParams.zookeeper.connect": "anistal:2181",
        "kafkaParams.group.id": "kafka-pruebas-daniel",
        "storageLevel": "MEMORY_AND_DISK_SER_2"
      }
    }
  ],
  "fields": [
    {
      "name": "c_r"
    },
    {
      "name": "ou_v"
    },
    {
      "name": "a"
    },
    {
      "type": "DateTime",
      "name": "r_a_m"
    },
    {
      "type": "GeoHash",
      "name": "geo"
    },
    {
      "name": "r_e_a"
    },
    {
      "name": "o"
    },
    {
      "name": "p_i"
    },
    {
      "name": "asset"
    }
  ],
  "cubes": [
    {
      "name": "assets",
      "dimensions": [
        {
          "dimension": "asset"
        }
      ]
    },
    {
      "name": "c_r_ou_v_a_r_a_m_o_p_i",
      "dimensions": [
        {
          "dimension": "c_r"
        },
        {
          "dimension": "ou_v"
        },
        {
          "dimension": "a"
        },
        {
          "dimension": "r_a_m",
          "precision": "minute"
        },
        {
          "dimension": "o"
        },
        {
          "dimension": "p_i"
        }
      ],
      "operators": [
        {
          "measureName": "range-operator",
          "type": "Range",
          "configuration": {
            "inputField": "o_long"
          }
        }
        ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {
        "multiplexer": "true",
        "isAutoCalculateId": "true"
      }
    }
  ],
  "parsers": [
    {
      "name": "detector-parser",
      "type": "Detector",
      "configuration": {
      }
    },
    {
      "name": "recorded_at_ms-parser",
      "type": "DateTime",
      "configuration": {
        "r_a_m": "unixMillis"
      }
    },
    {
      "name": "type-parser",
      "type": "Type",
      "configuration": {
        "sourceField": "o",
        "type": "Long",
        "newField": "o_long"
      }
    }
  ]
}