{
  "name": "socket_trigger_casting",
  "description": "Workflow with socket, trigger, casting and print",
  "settings": {
    "global": {
      "executionMode": "local",
      "streamTemporalTable": "stream",
      "driverUri": "/opt/sds/sparta/driver/sparta-driver.jar",
      "userPluginsJars": [
        {
          "jarPath": ""
        }
      ],
      "initSqlSentences": [
        {
          "sentence": ""
        }
      ]
    },
    "checkpointSettings": {
      "checkpointPath": "sparta/checkpoint",
      "autoDeleteCheckpoint": true,
      "addTimeToCheckpointPath": false
    },
    "streamingSettings": {
      "window": "6s"
    },
    "sparkSettings": {
      "master": "local[*]",
      "sparkKerberos": false,
      "sparkHome": "/opt/spark/dist",
      "submitArguments": {
        "userArguments": [
          {
            "submitArgument": "",
            "submitValue": ""
          }
        ],
        "deployMode": "client",
        "supervise": false
      },
      "sparkConf": {
        "sparkResourcesConf": {
          "coresMax": "2",
          "executorMemory": "1G",
          "executorCores": "1",
          "driverCores": "1",
          "driverMemory": "1G",
          "localityWait": "0",
          "taskMaxFailures": "8",
          "blockInterval": "200ms"
        },
        "sparkDockerConf": {
          "executorDockerImage": "qa.stratio.com/stratio/spark-krb-calico:2.1.0",
          "executorDockerVolumes": "/opt/mesosphere/packages/:/opt/mesosphere/packages/:ro,/opt/mesosphere/lib/:/opt/mesosphere/lib/:ro",
          "executorForcePullImage": false
        },
        "sparkMesosConf": {
          "mesosNativeJavaLibrary": "/opt/mesosphere/lib/libmesos.so",
          "mesosExecutorHome": "/opt/spark/dist"
        },
        "userSparkConf": [
          {
            "sparkConfKey": "",
            "sparkConfValue": ""
          }
        ],
        "coarse": true,
        "stopGracefully": true,
        "parquetBinaryAsString": true
      }
    }
  },
  "pipelineGraph": {
    "nodes": [
      {
        "name": "socket",
        "type": "WebSocketInputStep",
        "writer": {
          "autoCalculatedFields": [],
          "saveMode": "Append",
          "tableName": "inputsocket"
        },
        "configuration": {
          "url": "ws://stream.meetup.com/2/rsvps",
          "outputField": "raw",
          "outputType": "string"
        }
      },
      {
        "type": "TriggerTransformStep",
        "name": "trigger1",
        "writer": {
          "autoCalculatedFields": [],
          "saveMode": "Append",
          "tableName": "triggertable"
        },
        "configuration": {
          "whenError": "Error",
          "sql": "SELECT raw, array(named_struct('ean', raw, 'price', raw, 'lastUpdate', '')) as articles, array(map('raw', raw)) as articlesraw FROM socket"
        }
      },
      {
        "type": "CastingTransformStep",
        "name": "casting1",
        "writer": {
          "autoCalculatedFields": [],
          "saveMode": "Append",
          "tableName": "castingtable"
        },
        "configuration": {
          "whenError": "Error",
          "outputFieldsFrom": "FIELDS",
          "fields": [
            {"name": "raw", "type": "string"},
            {"name": "articlesraw", "type": "ArrayType(MapType(StringType,StringType,true),false)"},
            {"name": "articles", "type": "{\"type\":\"array\",\"elementType\":{\"type\":\"struct\",\"fields\":[{\"name\":\"ean\",\"type\":\"string\",\"nullable\":true,\"metadata\":{}},{\"name\":\"price\",\"type\":\"string\",\"nullable\":true,\"metadata\":{}},{\"name\":\"lastUpdate\",\"type\":\"string\",\"nullable\":false,\"metadata\":{}}]},\"containsNull\":false}"}
          ]
        }
      },
      {
        "name": "print",
        "type": "PrintOutputStep",
        "writer": {
          "autoCalculatedFields": [],
          "saveMode": "Append",
          "tableName": ""
        },
        "configuration": {
          "printData": true,
          "printSchema": true,
          "logLevel": "warn"
        }
      }
    ],
    "edges": [
      {
        "origin": "socket",
        "destination": "trigger1"
      },
      {
        "origin": "trigger1",
        "destination": "casting1"
      },
      {
        "origin": "casting1",
        "destination": "print"
      }
    ]
  }
}