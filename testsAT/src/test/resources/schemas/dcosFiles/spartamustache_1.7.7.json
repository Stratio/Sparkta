{
  "Framework": {
    "name": "sparta-server",
    "instances": 1,
    "cpus": 2,
    "mem": 2048,
    "force_pull_image": false,
    "run_mode": "production"
  },
  "Server": {
    "await_to_check_policy_status": "360s"
  },
  "Zookeeper": {
    "address": "zk-0001-zkuserland.service.paas.labs.stratio.com:2181,zk-0002-zkuserland.service.paas.labs.stratio.com:2181,zk-0003-zkuserland.service.paas.labs.stratio.com:2181",
    "connection_timeout": 15000,
    "session_timeout": 60000,
    "retry_attempts": 5,
    "retry_interval": 10000
  },
  "Marathon": {
    "uri": "https://newcore.labs.stratio.com/service/marathon",
    "api_version": "v2",
    "sparta_docker_image": "qa.stratio.com/stratio/sparta:1.7.7",
    "mesosphere_lib": "/opt/mesosphere/lib",
    "mesosphere_packages": "/opt/mesosphere/packages",
    "docker_forcePullImage": false,
    "docker_privileged": false,
    "gracePeriodSeconds": 240,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 3
  },
  "Calico": {
    "enabled": true,
    "calico_network": "stratio"
  },
  "Marathon-LB": {
    "enabled": true,
    "haproxy_group": "external",
    "haproxy_path": "/sparta-server",
    "haproxy_host": "sparta.newcore.labs.stratio.com",
    "sticky": false,
    "https": true,
    "server_side_verification": false,
    "cert_file": "/etc/ssl/cert.pem",
    "ca_file": "/etc/ssl/ca.bundle",
    "verifyhost": "sparta.marathon.mesos"
  },
  "Hdfs": {
    "generate_hdfs_conf_from_dfs": true,
    "generate_hdfs_conf_from_dfs_not_secured": false,
    "hdfs_conf_from_uri": false,
    "core_site_from_uri": false,
    "default_fs": "10.200.0.74",
    "conf_uri": "hdfs://10.200.0.74:8020",
    "user_name": "sparta-server"
  },
  "Security": {
    "Components": {
      "vault_enabled": true,
      "use_dynamic_authentication": true,
      "tls_enabled": true,
      "trustore_enabled": true,
      "kerberos_enabled": true,
      "oauth2_enabled": true,
      "ssl_enabled": true,
      "mesos_enabled": true,
      "gosec_enabled": true,
      "hdfs_enabled": true,
      "marathon_enabled": true
    },
    "Vault": {
      "host": "vault.service.paas.labs.stratio.com",
      "port": "8200",
      "instance_app_role": "open"
    },
    "Oauth": {
      "cookieName": "user",
      "authorize": "https://newcore.labs.stratio.com:9005/sso/oauth2.0/authorize",
      "accessToken": "https://newcore.labs.stratio.com:9005/sso/oauth2.0/accessToken",
      "profile": "https://newcore.labs.stratio.com:9005/sso/oauth2.0/profile",
      "logout": "https://newcore.labs.stratio.com:9005/sso/logout",
      "callback": "https://sparta.newcore.labs.stratio.com/sparta-server/login",
      "onLoginGoTo": "/sparta-server"
    },
    "GoSec": {
      "gosec_plugin_management_principal": "gosec-management",
      "gosec_plugin_sso_principal": "gosec-sso",
      "sparta_plugin_zk_connect": "gosec1.node.paas.labs.stratio.com:2181,gosec2.node.paas.labs.stratio.com:2181,gosec3.node.paas.labs.stratio.com:2181",
      "zk_watchers_enabled": true,
      "sparta_plugin_ldap_host": "ldap.integration.labs.stratio.com",
      "sparta_plugin_ldap_port": 636,
      "sparta_plugin_ldap_user_domain": "ou=People,dc=stratio,dc=com",
      "sparta_plugin_ldap_group_domain": "ou=Groups,dc=stratio,dc=com",
      "enable_sparta_plugin_cache": false,
      "sparta_plugin_kafka_bootstrap": "gosec1.node.paas.labs.stratio.com:9092,gosec2.node.paas.labs.stratio.com:9092,gosec3.node.paas.labs.stratio.com:9092",
      "sparta_plugin_kafka_topic": "audit"
    },
    "Mesos": {},
    "HDFS": {
      "hadoop_security_auth": "kerberos",
      "hadoop_rpc_protection": "authentication",
      "hadoop_dfs_encrypt_data_transfer": true,
      "hadoop_security_token_use_ip": false,
      "hadoop_namenode_krb_principal": "hdfs/10.200.0.74@DEMO.STRATIO.COM",
      "hadoop_namnode_krb_principal_pattern": "hdfs/*@DEMO.STRATIO.COM",
      "hadoop_dfs_encrypt_data_transfer_cipher_suites": "AES/CTR/NoPadding",
      "hadoop_dfs_encrypt_data_transfer_cipher_key_bitlength": "128",
      "key_tab_reload": false,
      "key_tab_reload_time": "23h"
    },
    "Marathon": {
      "sso_uri": "https://newcore.labs.stratio.com:9005/sso",
      "sso_clientId": "adminrouter_paas-master-1.node.paas.labs.stratio.com",
      "sso_redirectUri": "https://newcore.labs.stratio.com/acs/api/v1/auth/login"
    }
  },
  "Crossdata": {
    "Catalog": {
      "enable_catalog": true,
      "catalog_class": "org.apache.spark.sql.crossdata.catalyst.catalog.persistent.zookeeper.ZookeeperCatalog",
      "prefix": "crossdataCluster",
      "zookeeper_connection_string": "zk-0001-zkuserland.service.paas.labs.stratio.com:2181,zk-0002-zkuserland.service.paas.labs.stratio.com:2181,zk-0003-zkuserland.service.paas.labs.stratio.com:2181",
      "connection_timeout": 15000,
      "session_timeout": 60000,
      "retry_attempts": 5,
      "retry_interval": 10000
    },
    "Core": {
      "storage_path": "/tmp"
    },
    "Spark": {
      "master": "local[*]",
      "image": "qa.stratio.com/stratio/stratio-spark:2.1.0.1",
      "ui_enabled": true,
      "executor_memory": "1G",
      "driver_memory": "1G",
      "executor_cores": 1,
      "driver_cores": 1,
      "cores_max": 1,
      "token_cache_hdfs_enable": true,
      "datastore_tls_enable": true,
      "app_name": "sparta-server",
      "ca_name": "ca"
    }
  },
  "Frontend": {
    "timeout": 20000
  },
  "Java": {
    "heap_size": "-Xmx2048m",
    "heap_minimum_size": "-Xms1024m",
    "max_open_files": 65535
  },
  "Log": {
    "service_log_level": "ERROR",
    "sparta_log_level": "INFO",
    "spark_log_level": "ERROR",
    "hadoop_log_level": "ERROR",
    "zookeeper_log_level": "OFF",
    "parquet_log_level": "ERROR",
    "avro_log_level": "ERROR",
    "netty_log_level": "ERROR",
    "stderr_size": "120MB",
    "stdout_size": "120MB",
    "stderr_rotate": "rotate 10",
    "stdout_rotate": "rotate 10"
    "crossdata_log_level": "ERROR",
  }
}